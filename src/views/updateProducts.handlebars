<h1>Lista de productos viva</h1>
<div id="listContainer">
    {{#each products}}
        <div class="product" style="display: flex; gap: 1rem; align-items: center;">
            <h3>{{title}}</h3>
            <p>{{description}}</p>
            <p>Price: {{price}}</p>
            <p>id: {{id}}</p>
            <p>code :{{code}}</p>
            <p>Stock:{{stock}}</p>
        </div>
    {{/each}}
</div>

<script src="/socket.io/socket.io.js"></script>
<script src="/js/socket.js"></script>